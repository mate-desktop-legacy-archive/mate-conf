#: ../backends/evoldap-backend.c:587
#, c-format
msgid "Failed to contact LDAP server: %s"
msgstr "ཨེལ་ཌི་ཨེ་པི་སར་བར་: %s ལུ་འབྲེལ་བ་འཐབ་མ་ཚུགས།"

#: ../backends/evoldap-backend.c:675
#, c-format
msgid "Searching for entries using filter: %s"
msgstr "ཚགས་མ་:%s ལག་ལེན་འཐབ་ཐོག་ལས་ ཐོ་བཀོད་ཚུ་ འཚོལ་ཞིབ་འབད་དོ།"

#: ../backends/evoldap-backend.c:689
#, c-format
msgid "Error querying LDAP server: %s"
msgstr "ཨེལ་ཌི་ཨེ་པི་སར་བར་: %s དྲི་དཔྱད་འབད་ནི་ལུ་འཛོལ་བ།"

#: ../backends/evoldap-backend.c:699
#, c-format
msgid "Got %d entries using filter: %s"
msgstr "ཐོ་བཀོད་%d ཐོབ་ཅི་ ཚགས་མ་: %s ལག་ལེན་འཐབ་སྟེ།"

#: ../backends/mateconf-merge-tree.c:59
#, c-format
msgid "Cannot find directory %s\n"
msgstr "སྣོད་ཐོ་%sའཚོལ་མི་ཐོབ་པས། \n"

#: ../backends/mateconf-merge-tree.c:75
#, c-format
msgid "Error saving MateConf tree to '%s': %s\n"
msgstr "ཇི་ཀཱོནཕི་རྩ་འབྲེལ་ '%s': %s ལུ་ བསྲུང་ནི་ལུ་འཛོལ་བ།\n"

#: ../backends/mateconf-merge-tree.c:101
#, c-format
msgid "Usage: %s <dir>\n"
msgstr "ལག་ལེན་: %s <dir>\n"

#: ../backends/mateconf-merge-tree.c:107
#, c-format
msgid ""
"Usage: %s <dir>\n"
"  Merges a markup backend filesystem hierarchy like:\n"
"    dir/%%mateconf.xml\n"
"        subdir1/%%mateconf.xml\n"
"        subdir2/%%mateconf.xml\n"
"  to:\n"
"    dir/%%mateconf-tree.xml\n"
msgstr "ལག་ལེན་: %s <dir>\n འདི་བཟུམ་མའི་ རྟགས་བཀོད་རྒྱབ་མཐའི་ ཡིག་སྣོད་རིམ་ལུགས་ཀྱི་སྡེ་རིམ་ཅིག་ གཅིག་སྒྲིལ་འབདཝ་ཨིན། \n སྣོད་ཐོ་ཇི་ཀཱོནཕི་ ཨེགསི་ཨེམ་ཨེལ་། \nཡན་ལག་སྣོད་ཐོ་༡ /%% ཇི་ཀཱོནཕི་ ཨེགསི་ཨེམ་ཨེལ་། \n\"ཡན་ལག་སྣོད་ཐོ་༢ /%% ཇི་ཀཱོནཕི་ ཨེགསི་ཨེམ་ཨེལ་། \nལུ་: \n སྣོད་ཐོ་/%% ཇི་ཀཱོནཕི་-ཊི་ཨེགསི་ཨེམ་ཨེལ།\n"

