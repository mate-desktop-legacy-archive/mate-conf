"    kataloog/%%mateconf.xml\n"
"        alamkataloog1/%%mateconf.xml\n"
"        alamkataloog2/%%mateconf.xml\n"
"  tulemus:\n"
"    kataloog/%%mateconfi-puu.xml\n"

